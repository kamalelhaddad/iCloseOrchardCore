@page "/liveEditor/{id}"

@inject OrchardCore.IOrchardHelper Orchard

@{
    var livePage = await Orchard.GetContentItemByIdAsync(Id);
}


<div class="container text-center">
    <h1>This is the blog post: @livePage.DisplayText</h1>
    <hr/>
    @Orchard.ConsoleLog(Json.Serialize(livePage))
</div>

@functions
{
    [FromRoute]
    public string Id { get; set; }
}